// app global css

border-radius()
  -webkit-border-radius: arguments
  -moz-border-radius: arguments
  border-radius: arguments

square()
  height: arguments
  width: arguments

.round_img
  border-radius(100%)

.icon
  height: 50px;
  width: 50px;

.square_small
  square(200px)

.my-card
  width 100%
  max-width 250px
